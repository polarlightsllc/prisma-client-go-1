datasource db {
  provider = "sqlite"
  url      = env("__REPLACE__")
}

generator db {
  provider         = "go run github.com/steebchen/prisma-client-go-1"
  output           = "."
  disableGitignore = true
  package          = "binaries"
  binaryTargets    = ["native", "debian-openssl-1.1.x"]
}

model User {
  id       String  @id @default(cuid()) @map("_id")
  email    String  @unique
  username String
  name     String?
  stuff    String?
}
