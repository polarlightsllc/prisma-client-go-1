datasource db {
  provider = "sqlite"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/steebchen/prisma-client-go-1"
  output            = "."
  disableGoBinaries = true
  package           = "pagination"
  previewFeatures   = ["mongoDb"]
}

model Post {
  id      String @id @default(cuid()) @map("_id")
  title   String @unique
  content String
}
