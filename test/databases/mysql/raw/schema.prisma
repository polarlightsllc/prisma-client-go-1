datasource db {
  provider = "mysql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/polarlightsllc/prisma-client-go-1"
  output            = "."
  disableGoBinaries = true
  package           = "raw"
}

model User {
  id       String    @id @default(cuid())
  email    String    @unique
  username String
  str      String
  strOpt   String?
  date     DateTime
  dateOpt  DateTime?
  int      Int
  intOpt   Int?
  float    Float
  floatOpt Float?
  bool     Boolean
  boolOpt  Boolean?
}
