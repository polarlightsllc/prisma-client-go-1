datasource db {
  provider = "mysql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/steebchen/prisma-client-go-1"
  output            = "."
  disableGoBinaries = true
  package           = "db"
  previewFeatures   = ["filterJson"]
}

model User {
  id      String @id @default(cuid())
  json    Json
  jsonOpt Json?
}
