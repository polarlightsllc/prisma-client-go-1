datasource db {
  provider = "sqlite"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/steebchen/prisma-client-go-1"
  output            = "."
  disableGoBinaries = true
  package           = "composite"
  previewFeatures   = ["mongoDb"]
}

model User {
  id  String @default(cuid()) @id @map("_id")
  foo String
  bar String

  @@unique([foo, bar])
}

model Post {
  id  String @default(cuid()) @id @map("_id")
  foo String
  bar String

  @@unique([foo, bar])
}
