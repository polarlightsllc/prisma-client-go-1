datasource db {
  provider = "postgresql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/steebchen/prisma-client-go-1"
  output            = "."
  disableGoBinaries = true
  package           = "db"
}

model User {
  id    String  @id @default(cuid())
  email String
  name  String?
}
